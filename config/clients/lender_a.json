{
  "client_id": "LENDER_A",
  "client_name": "Lender A Financial",
  "description": "Daily loan data feed from Lender A in CSV format",
  "file_format": "csv",
  "delimiter": ",",
  "has_header": true,
  "encoding": "utf-8",

  "date_formats": [
    "YYYY-MM-DD",
    "MM/DD/YYYY",
    "M/D/YYYY"
  ],

  "status_code_mapping": {
    "A": "ACTIVE",
    "C": "CLOSED",
    "D": "DELINQUENT",
    "P": "PAID_OFF"
  },

  "ingestion_settings": {
    "allow_partial_records": false,
    "reject_on_schema_mismatch": true,
    "max_error_percentage": 10
  },

  "contact": {
    "team": "Data Operations",
    "email": "dataops@lendera.example"
  }
}
